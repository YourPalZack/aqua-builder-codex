{
  "name": "@aquabuilder/scraper",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "dev": "node -e \"if(!process.env.REDIS_URL){console.log('scraper: REDIS_URL not set; dev disabled');process.exit(0)}else{require('node:child_process').spawn('node',['--loader','ts-node/esm','src/index.ts'],{stdio:'inherit'})}\"",
    "start": "node dist/index.js",
    "build": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "@prisma/client": "^6.16.3",
    "bullmq": "^5.30.1",
    "ioredis": "^5.4.2",
    "node-fetch": "^3.3.2",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "typescript": "^5.5.0"
  }
}
