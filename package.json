{
  "name": "aquabuilder-monorepo",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "devDependencies": {
    "@types/node": "^24.6.2",
    "eslint": "^9.37.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.6",
    "turbo": "^2.5.8",
    "typescript": "^5.9.3"
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "services/*"
  ],
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "qa": "bash -lc 'cd tools/qa && tsx src/qa-agent.ts'"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "zod": "^4.1.11"
  }
}
